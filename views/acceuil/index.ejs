<% include ../layouts/doctype.ejs %>


<div class="jumbotron jumbotron-fluid text-white" id="header-jumbotron">
  <div class="container">
    <h1 class="display-4">Fluid jumbotron</h1>
    <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.</p>
  </div>
</div>






<div class="mx-auto d-flex justify-content-center p-5">
  <div class="card-deck">

    <% categories.forEach(function(categorie){ %>
       <% if (categorie.active == 1) { %>
          <%- include('partials/cardCategorieArticles', {categorie: categorie}); %>
       <% } %>
    <% }); %>

  </div>
</div>

















<% include ../layouts/doctype_end.ejs %>